<!DOCTYPE html>
<html lang="en">
    <head>
    <meta charset="utf-8">
        <title>Great Number Game</title>
        <link rel="stylesheet" href="Content/style.css"/>
    </head>
    <body>
        <div class="wrapper">
            <h1>Here are the awesome quotes!</h1>
            
            @Each
            <div class="quote">
                <p>"@Current.quote"</p>
                <p>- @Current.name at @Current.created_at</p>
            </div>
            @EndEach
        </div>
    </body>
</html>
